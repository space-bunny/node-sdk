{
  "name": "spacebunny",
  "version": "0.7.11",
  "description": "NodeJS client library for Space Bunny IoT platform",
  "main": "lib/spacebunny.js",
  "scripts": {
    "flow": "flow",
    "flow-typed": "flow-typed",
    "debug": "cross-env NODE_ENV=development DEBUG=* node -r babel-register index.js",
    "start:dev": "cross-env NODE_ENV=development node -r babel-register index.js",
    "start:prod": "cross-env NODE_ENV=production node -r babel-register index.js",
    "start:samples": "serve ./ -l 8080",
    "start:node-sample": "cross-env NODE_ENV=production node -r source-map-support/register",
    "remove-lib": "rimraf ./lib",
    "clean-lib": "npm run remove-lib && mkdir lib",
    "prebuild": "npm run clean-lib",
    "build": "cross-env NODE_ENV=production babel-node tools/build.js",
    "docs": "echo TODO gulp docs'"
  },
  "bin": {
    "node-dev": "./node_modules/.bin/node-dev",
    "babel-node": "./node_modules/.bin/babel-node",
    "flow": "./node_modules/.bin/flow",
    "flow-typed": "./node_modules/.bin/flow-typed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/space-bunny/node-sdk.git"
  },
  "keywords": [
    "spacebunny",
    "iot"
  ],
  "author": "Fancy Pixel (http://fancypixel.it)",
  "license": "MIT",
  "dependencies": {
    "amqplib": "0.5.5",
    "axios": "0.19.0",
    "bluebird": "3.5.5",
    "humps": "2.0.1",
    "js-md5": "0.7.3",
    "lodash": "4.17.15",
    "mqtt": "2.18.8",
    "stompjs": "git://github.com/gfoiani/stomp-websocket.git#2.4.0",
    "underscore": "1.9.1",
    "url-join": "4.0.1",
    "uuid-v4": "0.1.0"
  },
  "devDependencies": {
    "ajv": "6.10.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-runtime": "6.26.0",
    "chalk": "2.4.2",
    "cross-env": "5.2.0",
    "eslint": "6.2.2",
    "flow-bin": "0.100.0",
    "flow-typed": "2.5.1",
    "minimist": "1.2.0",
    "rimraf": "2.6.3",
    "serve": "11.1.0",
    "source-map-support": "0.5.13",
    "string-replace-loader": "2.2.0",
    "webpack": "4.36.1",
    "webpack-md5-hash": "0.0.6"
  }
}
