{
  "name": "spacebunny",
  "version": "0.7.15",
  "description": "NodeJS client library for Space Bunny IoT platform",
  "main": "lib/spacebunny.js",
  "scripts": {
    "flow": "flow",
    "flow-typed": "flow-typed",
    "debug": "cross-env NODE_ENV=development DEBUG=* node -r babel-register index.js",
    "start:dev": "cross-env NODE_ENV=development node -r babel-register index.js",
    "start:prod": "cross-env NODE_ENV=production node -r babel-register index.js",
    "start:samples": "serve ./ -l 8080",
    "start:node-sample": "cross-env NODE_ENV=production node -r source-map-support/register",
    "remove-lib": "rimraf ./lib",
    "clean-lib": "npm run remove-lib && mkdir lib",
    "prebuild": "npm run clean-lib",
    "build": "cross-env NODE_ENV=production babel-node tools/build.js",
    "docs": "echo TODO gulp docs'",
    "generate-stats": "ANALYZER=true webpack --config webpack.config.prod.babel.js --profile --json > stats.json",
    "serve-stats": "webpack-bundle-analyzer --port 4200 stats.json",
    "analyze-bundle": "rimraf stats.json && npm run generate-stats && npm run serve-stats"
  },
  "bin": {
    "node-dev": "./node_modules/.bin/node-dev",
    "babel-node": "./node_modules/.bin/babel-node",
    "flow": "./node_modules/.bin/flow",
    "flow-typed": "./node_modules/.bin/flow-typed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/space-bunny/node-sdk.git"
  },
  "keywords": [
    "spacebunny",
    "iot"
  ],
  "author": "Fancy Pixel (http://fancypixel.it)",
  "license": "MIT",
  "dependencies": {
    "amqplib": "0.5.5",
    "axios": "0.19.2",
    "bluebird": "3.7.2",
    "humps": "2.0.1",
    "js-md5": "0.7.3",
    "lodash": "4.17.19",
    "mqtt": "2.18.8",
    "stompjs": "git://github.com/gfoiani/stomp-websocket.git#2.4.0",
    "url-join": "4.0.1",
    "uuid-v4": "0.1.0"
  },
  "devDependencies": {
    "ajv": "6.11.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-runtime": "6.26.0",
    "chalk": "3.0.0",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "flow-bin": "0.100.0",
    "flow-typed": "2.5.1",
    "minimist": "1.2.0",
    "rimraf": "3.0.2",
    "serve": "11.3.0",
    "source-map-support": "0.5.16",
    "string-replace-loader": "2.2.0",
    "webpack": "4.41.6",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-md5-hash": "0.0.6"
  }
}
