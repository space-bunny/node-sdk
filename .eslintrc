{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "no-console": 1,
    "semi": 2,
    "no-control-regex": 0,
    "no-undef": 0,
    "react/require-default-props": 0
  },
  "plugins": [
  ]
}
